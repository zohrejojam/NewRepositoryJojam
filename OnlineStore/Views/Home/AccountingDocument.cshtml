


<table class="table table-bordered" id="list">
    <tr>
        <th>MaterialCode</th>
        <th>MaterialTitle</th>
        <th>MaterialGroupName</th>
        <th>MinInventory</th>
        <th>Number</th>
        <th>Status</th>
    </tr>
</table>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $.getJSON("/Api/StoreHouse", function (res) {
        $.each(res, function (key, val) {
            var item = "<tr><td>" + val.MaterialCode + "</td><td>" + val.MaterialTitle + "</td><td>" + val.MaterialGroupName + "</td><td>" + val.MinInventory + "</td><td>" + val.Number + "</td><td>" + val.Status + "</td></tr>";
            $("#list").append(item);

        });
    });



</script>

