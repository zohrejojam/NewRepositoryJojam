<table class="table table-bordered" id="list">
    <tr>
        <th>materialId</th>
        <th>number</th>
        <th>date</th>
        <th>inputInvoiceNumber</th>
    </tr>
</table>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $.getJSON("/Api/StoreHouse", function (res) {
        $.each(res, function (key, val) {
            var item = "<tr><td>" + val.materialId + "</td><td>" + val.number + "</td><td>" + val.date + "</td><td>" + val.inputInvoiceNumber + "</td></tr>";
            $("#list").append(item);

        });
    });



</script>
