<table class="table table-bordered" id="list">
    <tr class="table-bordered">
        <th>Code</th>
        <th>Title</th>
        <th>MaterialGroup</th>
        <th>Min Inventory</th>
        <th>Command</th>
    </tr>
</table>


<script>
    $.getJSON("/Api/Materials", function(res) {
        $.each(res, function(key, val) {
            var item = "<tr><td>" + val.MaterialCode + "</td><td>" + val.MaterialTitle + "</td><td>" + val.MaterialGroupId + "</td><td>" + val.MinInventory  + "</td></tr>";
            $("#list").append(item);

        });
    })



</script>
